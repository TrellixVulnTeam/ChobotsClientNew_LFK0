{
  "name": "chobotsplayer",
  "icon": "favicon.ico",
  "version": "1.0.0",
  "description": "Desktop client for Chobots.world",
  "main": "./build/main.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && electron .",
    "make-win": "electron-builder build --win --publish never",
    "make-mac": "electron-builder build --mac --publish never",
    "make-win-publish": "electron-builder build --win --publish onTagOrDraft",
    "make-mac-publish": "electron-builder build --mac --publish onTagOrDraft"
  },
  "devDependencies": {
    "@types/electron-store": "^3.2.0",
    "electron": "^8.5.5",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@types/discord-rpc": "^4.0.0",
    "@types/node": "^17.0.8",
    "@types/request": "^2.48.8",
    "@types/uuid": "^8.3.4",
    "discord-rpc": "^4.0.1",
    "electron-store": "^8.0.1",
    "esm": "^3.2.25",
    "request": "^2.88.2"
  },
  "bugs": {
    "url": "https://github.com/sheenieboy/ChobotsClientNew"
  },
  "homepage": "https://github.com/sheenieboy/ChobotsClientNew",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sheenieboy/ChobotsClientNew.git"
  },
  "author": "Chobots, Inc",
  "license": "BSD-2-Clause",
  "build": {
    "appId": "com.chobots.desktop",
    "productName": "Chobots Client",
    "icon": "pages/assets/icons/icon.png",
    "asar": "false",
    "files": ["!src/*", "./build", "./pages", "./flashPlugin", "./ABOUT.txt"],
    "win": {
      "publish": "github"
    },
    "mac": {
      "publish": "github"
    },
    "dmg": {
      "window": {
        "width": "540",
        "height": "380"
      }
    }
  },
  "keywords": []
}
